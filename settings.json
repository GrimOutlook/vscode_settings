{
  // Language specific settings
  "[gitignore]": {
    "rewrap.autoWrap.enabled": false,
    "rewrap.onSave": false
  },
  "[gradle]": {
    "rewrap.autoWrap.enabled": false,
    "rewrap.onSave": false
  },
  "[ignore]": {
    "rewrap.autoWrap.enabled": false,
    "rewrap.onSave": false
  },
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[javascriptreact]": {
    "editor.tabSize": 2
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.tabSize": 2
  },
  "[vue]": {
    "editor.tabSize": 2
  },
  "[xml]": {
    "editor.autoClosingBrackets": "never",
    "editor.defaultFormatter": "redhat.vscode-xml",
    "files.trimFinalNewlines": true
  },
  "[xslt]": {
    "editor.semanticHighlighting.enabled": true
  },
  "chat.commandCenter.enabled": false,
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/{info,lfs,logs,refs,objects}/**",
    ".git/{index,*refs,*HEAD}",
    ".vscode",
    ".vscode-insiders",
    "*.svg"
  ],
  "cSpell.userWords": [
    "direnv",
    "envrc",
    "IMAPB",
    "manganis",
    "MISB",
    "nixpkgs",
    "pids",
    "pkgs",
    "println",
    "STANAG",
    "usize",
    "Zulip"
  ],
  "diffEditor.experimental.showMoves": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "diffEditor.maxFileSize": 100,
  "diffEditor.renderSideBySide": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.foldingMaximumRegions": 50000,
  "editor.formatOnSave": true,
  "editor.largeFileOptimizations": false,
  "editor.maxTokenizationLineLength": 2000000,
  "editor.minimap.autohide": true,
  "editor.minimap.enabled": false,
  "editor.rulers": [
    80
  ],
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 80,
  "editor.wrappingIndent": "same",
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.autoCheckUpdates": true,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "afterDelay",
  "files.trimTrailingWhitespace": true,
  "firedbg.showWelcome": false,
  "git-identity.authors": [
    "Dominic Grimaldi dominic.j.grimaldi@gmail.com",
    "Dominic Grimaldi dominic.j.grimaldi2.ctr@army.mil"
  ],
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.ignoreLegacyWarning": true,
  "gremlins.showInProblemPane": true,
  "hediet.vscode-drawio.resizeImages": null,
  "hexeditor.columnWidth": 18,
  "hexeditor.defaultEndianness": "little",
  "hexeditor.inspectorType": "aside",
  "hexeditor.showDecodedText": true,
  "html.format.wrapLineLength": 80,
  "javascript.suggest.paths": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jest.jestCommandLine": "pnpm test",
  "markdownlint.config": {
    "MD024": {
      "siblings_only": true
    }
  },
  "prettier.printWidth": 80,
  "projectManager.git.baseFolders": [
    "/home/dom/",
    "C:\\Users\\GrimaldiDJ\\Code"
  ],
  "redhat.telemetry.enabled": false,
  "rewrap.autoWrap.enabled": true,
  "rewrap.doubleSentenceSpacing": false,
  // "rewrap.onSave": true,
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.checkOnSave": true,
  "search.useGlobalIgnoreFiles": true,
  "search.useIgnoreFiles": true,
  "security.workspace.trust.untrustedFiles": "open",
  "tailwindCSS.experimental.classRegex": [
    "class: \"(.*)\""
  ],
  "tailwindCSS.includeLanguages": {
    "rust": "html"
  },
  "terminal.integrated.scrollback": 10000,
  "typescript.suggest.paths": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.customTitleBarVisibility": "windowed",
  "workbench.colorCustomizations": {
    "editorRuler.foreground": "#353535"
  },
  "workbench.colorTheme": "Gruvbox Dark Hard",
  "workbench.editor.enablePreview": true,
  "workbench.editorAssociations": {
    "*.copilotmd": "vscode.markdown.preview.editor",
    "*.svg": "hediet.vscode-drawio-text"
  },
  "workbench.editorLargeFileConfirmation": 100,
  "workbench.iconTheme": "gruvbox-material-icon-theme",
  "workbench.panel.showLabels": false,
  "workbench.startupEditor": "none",
  "xml.foldings.includeClosingTagInFold": true,
  "xml.symbols.maxItemsComputed": 50000
}